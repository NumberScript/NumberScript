{
  "name": "numberscript",
  "displayName": "NumberScript",
  "description": "A symbolic math language for VS Code Web with simplify, expand, solve, and graph support.",
  "version": "1.1.15",
  "publisher": "NumberScript",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Programming Languages",
    "Education",
    "Visualization"
  ],
  "icon": "resources/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/numberscript/numberscript"
  },
  "bugs": {
    "url": "https://github.com/numberscript/numberscript/issues"
  },
  "homepage": "https://github.com/numberscript/numberscript",
  "keywords": [
    "math",
    "algebra",
    "symbolic",
    "solve",
    "graphing",
    "education",
    "numberscript"
  ],
  "activationEvents": [
    "onLanguage:numberscript",
    "onCommand:numberscript.run"
  ],
  "main": "./dist/extension.client.js",
  "scripts": {
    "clean": "rm -rf dist .webpack .cache || true",
    "build": "webpack --mode=production --config webpack.config.js",
    "dev": "webpack --mode=development --watch --config webpack.config.js",
    "lint": "eslint src --ext .ts",
    "package": "npx @vscode/vsce package",
    "publish": "npx @vscode/vsce publish -p $VSCE_TOKEN"
  },
  "devDependencies": {
    "@types/node": "^20.19.5",
    "@types/vscode": "^1.87.0",
    "copy-webpack-plugin": "^11.0.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.0.0",
    "webpack": "^5.101.3",
    "webpack-cli": "^5.0.0",
    "worker-loader": "^3.0.8",
    "nearley-loader": "^2.0.0",
    "style-loader": "^3.3.2",
    "css-loader": "^6.8.1",
    "raw-loader": "^4.0.2"
  },
  "dependencies": {
    "mathjs": "^11.8.0",
    "nerdamer": "^1.1.13",
    "mathsteps": "^0.2.0",
    "nearley": "^2.20.1"
  }
}
