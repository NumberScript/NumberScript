{
  "scopeName": "source.numberscript",
  "patterns": [
    { "name": "keyword.control.numberscript", "match": "\\b(simplify|expand|combineLikeTerms|solve|plot)\\b" },
    { "name": "support.function.numberscript", "match": "\\b(sin|cos|tan|exp|log|sqrt|abs)\\b" },
    { "name": "constant.numeric.numberscript", "match": "\\b[0-9]+(\\.[0-9]+)?\\b" },
    { "name": "variable.other.numberscript", "match": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b" },
    { "name": "comment.line.number", "match": "#.*$" }
  ]
}
